                              <rng:ref name="model.global"/>
                           </rng:zeroOrMore>
                        </oneOrMore>
                        <rng:zeroOrMore>
                           <rng:choice>
                              <rng:ref name="model.divLike"/>
                              <rng:ref name="model.divGenLike"/>
                           </rng:choice>
                           <rng:zeroOrMore>
                              <rng:ref name="model.global"/>
                           </rng:zeroOrMore>
                        </rng:zeroOrMore>
                     </group>
                  </rng:choice>
                  <rng:zeroOrMore>
                     <rng:ref name="model.divBottom"/>
                     <rng:zeroOrMore>
